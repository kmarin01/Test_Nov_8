BOB_808.bob_getCookie=function(d){var b=d+"=";var a=document.cookie.split(";");for(var e=0;e<a.length;e++){var f=a[e];while(f.charAt(0)==" "){f=f.substring(1)}if(f.indexOf(b)==0){return f.substring(b.length,f.length)}}return""};BOB_808.bob_setCookie=function(c,f,b){var e=new Date();e.setTime(e.getTime()+b);var a="expires="+e.toUTCString();document.cookie=c+"="+f+"; "+a};BOB_808.get_bob_session=function(){var a=BOB_808.bob_getCookie("bob_session_id");if(a===""){a=BOB_808.get_cache_buster();BOB_808.bob_setCookie("bob_session_id",a,3600000)}return a};BOB_808.get_cache_buster=function(){return((new Date()).getTime()+""+Math.floor(Math.random()*1000000)).substr(0,16)};